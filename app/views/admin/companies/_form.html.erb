<%= simple_form_for @admin_company, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :name %>
  <%= f.input :trading_name %>
  <%= f.input :registration_number %>
  <%= f.input :address %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                admin_companies_path, :class => 'btn btn-default' %>
<% end %>
